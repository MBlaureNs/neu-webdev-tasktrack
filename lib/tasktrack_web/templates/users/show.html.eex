<h2>Show Users</h2>

<ul>

  <li>
    <strong>Username:</strong>
    <%= @users.username %>
  </li>

  <li>
    <strong>Name:</strong>
    <%= @users.name %>
  </li>

</ul>

<span><%= link "Edit", to: users_path(@conn, :edit, @users) %></span>
<span><%= link "Back", to: users_path(@conn, :index) %></span>
